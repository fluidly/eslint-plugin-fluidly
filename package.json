{
  "name": "eslint-plugin-fluidly",
  "description": "eslint plugins for the Fluidly codebase",
  "version": "1.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fluidly/eslint-plugin-fluidly"
  },
  "keywords": [
    "moment-js",
    "date-fns",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "miketownsend",
  "main": "rules/index.js",
  "scripts": {
    "test:watch": "jest --watch",
    "test": "jest --coverage",
    "precommit": "pretty-quick --staged"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "date-fns": "^2.15.0",
    "eslint": "^7.7.0",
    "jest": "^26.4.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "dependencies": {
    "kebab-case": "^1.0.0"
  },
  "peerDependencies": {
    "eslint": "^5.5.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "./tests/performance.js"
    ]
  },
  "license": "MIT"
}
